<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--  【统一颜色定义区域】 -->
    <color name="alpha_background_window">#FFf3f3f3</color>                  <!-- 窗体的背景色 -->
    <color name="alpha_background_title">@android:color/white</color>        <!-- 标题栏颜色 -->


    <color name="alpha_divider_color">#FFEAEAEA</color>                        <!-- 分割线 -->
    <color name="alpha_font_color_title">#FF323232</color>                     <!-- 标题字体颜色-->
    <color name="alpha_font_color_black">#FF4A4A4A</color>                   <!-- 普通黑色-->
    <color name="alpha_font_color_gray">#FF8c8f92</color>                    <!-- 普通灰色 -->
    <color name="alpha_font_color_orange">#FFed6c00</color>


    <!--  【统一单位定义区域】 -->
    <dimen name="alpha_height_divider">0.5dp</dimen>          <!-- 分割线高度 -->
    <dimen name="alpha_height_title">44dp</dimen>            <!-- 页面标题布局高度 -->

    <!-- 字体大小相对单位 为zeplin 聚合单位 [四舍五入]   -->
    <dimen name="alpha_font_size_title">18sp</dimen>            <!-- 页面标题字体大小 -->
    <dimen name="alpha_font_size_comm">16sp</dimen>             <!-- 页面普通字体大小 -->
    <dimen name="alpha_font_size_comm_medium">15sp</dimen>      <!-- 页面普通小字体 -->

    <dimen name="alpha_font_size_mini">14sp</dimen>             <!-- 页面小字体 -->
    <dimen name="alpha_font_size_mini_medium">13sp</dimen>      <!-- 页面中等小字体 -->
    <dimen name="alpha_font_size_mini_lowest">12sp</dimen>      <!-- 页面最小字体 -->

    <!-- 布局的内容边距 常见左边距;右边距; -->
    <dimen name="alpha_padding_content">15dp</dimen>          <!-- 内容边距 -->

</resources>